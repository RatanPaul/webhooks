<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Holds all the query string webhook endpoints</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Path__c</fullName>
        <caseSensitive>false</caseSensitive>
        <description>Path matched on the incoming request</description>
        <externalId>true</externalId>
        <inlineHelpText>What is the path of this webhook?</inlineHelpText>
        <label>Path</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Response__c</fullName>
        <description>Served as the HTTP response body</description>
        <externalId>false</externalId>
        <inlineHelpText>What text should be served as the webhook response?</inlineHelpText>
        <label>Response</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Query String Webhook Config</label>
    <visibility>Public</visibility>
</CustomObject>
